{
  "name": "mehr-it/lara-sqs-plain",
  "description": "Receive custom/plain SQS messages in Laravel",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
